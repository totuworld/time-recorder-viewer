{
  "name": "time-recorder-viewer",
  "version": "0.1.25",
  "license": "MIT",
  "repository": "github:totuworld/time-recorder-viewer",
  "scripts": {
    "start:dev": "razzle start",
    "start:debug": "razzle start --inspect",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start": "NODE_ENV=production node ./build/server.js",
    "start:onlinux": "npm run build && NODE_ENV=production pm2 start build/server.js -i 2 --name YAWORK_WEB",
    "dist": "node dist.js",
    "lint": "tslint -c tslint.json \"src/**/*.ts*\""
  },
  "dependencies": {
    "@coreui/icons": "^0.3.0",
    "@jaredpalmer/after": "^1.4.0",
    "@slack/web-api": "^5.6.0",
    "ajv": "^6.5.2",
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.2",
    "classnames": "^2.2.6",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "firebase": "^7.5.0",
    "formik": "^1.5.1",
    "glamor": "^2.20.40",
    "immer": "^8.0.1",
    "is-docker": "^1.1.0",
    "luxon": "^1.3.3",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dates": "^21.5.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.1.1",
    "recharts": "^1.8.5",
    "serialize-javascript": "^2.1.1",
    "shelljs": "^0.8.2",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chart.js": "^2.7.57",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/luxon": "^1.2.2",
    "@types/node": "^8.10.59",
    "@types/react": "^16.9.13",
    "@types/react-dates": "^17.1.5",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/reactstrap": "^8.0.6",
    "@types/recharts": "^1.8.3",
    "@types/shelljs": "^0.8.0",
    "@types/urijs": "^1.19.4",
    "@types/webpack-env": "^1.13.6",
    "razzle": "^3.0.0",
    "razzle-plugin-typescript": "^3.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
