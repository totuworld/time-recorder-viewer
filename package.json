{
  "name": "time-recorder-viewer",
  "version": "0.1.25",
  "license": "MIT",
  "scripts": {
    "start:dev": "razzle start",
    "start:debug": "razzle start --inspect",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start": "NODE_ENV=production node build/server.js",
    "dist": "node dist.js",
    "lint": "tslint -c tslint.json \"src/**/*.ts*\""
  },
  "dependencies": {
    "@coreui/coreui": "^2.0.4",
    "@coreui/icons": "^0.3.0",
    "@coreui/react": "^2.0.5",
    "@jaredpalmer/after": "^1.3.1",
    "@slack/client": "^4.8.0",
    "@types/chart.js": "^2.7.30",
    "@types/luxon": "^1.2.2",
    "ajv": "^6.5.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.2",
    "classnames": "^2.2.6",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "firebase": "^5.3.1",
    "glamor": "^2.20.40",
    "immer": "^1.7.2",
    "is-docker": "^1.1.0",
    "luxon": "^1.3.3",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "moment": "^2.22.2",
    "react": "^16.2.0",
    "react-chartjs-2": "^2.7.4",
    "react-dates": "^17.1.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^6.3.1",
    "recharts": "^1.1.0",
    "shelljs": "^0.8.2",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.11.1",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.11",
    "@types/react-dates": "^17.1.0",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/reactstrap": "^6.0.2",
    "@types/recharts": "^1.0.24",
    "@types/shelljs": "^0.8.0",
    "@types/urijs": "^1.15.38",
    "@types/webpack-env": "^1.13.6",
    "razzle": "^2.4.0",
    "razzle-plugin-typescript": "^2.2.0",
    "ts-jest": "^22.4.5",
    "ts-loader": "^4.2.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "2.9.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
